{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Dotlib.Encryption.Tests/Dotlib.Encryption.Tests.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "pack",
            "command": "dotnet",
            "type": "process",
            "args": [
                "pack",
                "-c",
                "Release",
                "-o",
                "out",
                "${workspaceFolder}/Dotlib.Encryption/Dotlib.Encryption.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        }
    ]
}